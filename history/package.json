  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.12",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2",
    "uglify-es": "^3.3.9"
  },
  "peerDependencies": {
    "simple-data-store": "^3.0.0"
  }
