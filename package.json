{
  "name": "simple-data-store",
  "version": "2.0.1",
  "description": "A simple data store for state management.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": "github:astrellon/simple-data-store",
  "scripts": {
    "build": "tsc && uglifyjs -o dist/index.min.js --source-map filename=dist/input.js.map dist/index.js"
  },
  "keywords": [
    "state",
    "immutable"
  ],
  "npmName": "simple-data-store",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "author": "Alan Lawrey",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^3.5.2",
    "uglify-es": "^3.3.9"
  }
}
