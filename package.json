    "@types/redux": "^3.6.0",
    "benchmark": "^2.1.4",
    "jest": "^24.8.0",
    "redux": "^4.0.4",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2",
    "uglify-es": "^3.3.9"
  }
