{
  "name": "simple-data-store",
  "version": "3.2.2",
  "description": "A simple data store for state management.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": "github:astrellon/simple-data-store",
  "scripts": {
    "build": "tsc && terser -c -m toplevel,reserved=['DataStore','EmptyModifier'] --mangle-props builtins,regex=/currentState\\|subscriptions\\|prevValue\\|selector\\|comparer\\|getValue\\|name\\|checkIfChanged/ -o dist/index.min.js --source-map filename=dist/input.js.map dist/index.js",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "keywords": [
    "state",
    "immutable"
  ],
  "npmName": "simple-data-store",
  "author": "Alan Lawrey",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "jest": "^26.0.1",
    "terser": "^4.8.0",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5",
    "uglify-es": "^3.3.9"
  }
}
